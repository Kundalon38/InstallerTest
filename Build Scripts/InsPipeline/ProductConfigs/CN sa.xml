<BuildProcess>
  <ProductName>RISAConnection</ProductName>
  <Abbrev2>CN</Abbrev2>
  <InstallType>Standalone</InstallType>
  <BuildEnabled>Y</BuildEnabled>
  <NotifyOnSuccessEmails>mattl@risa.com</NotifyOnSuccessEmails>
  <EnableSuccessEmail>false</EnableSuccessEmail>
  <PublishInstaller_Directory>G:\Shared drives\Support Team\Install_packages\RISAConnection</PublishInstaller_Directory>
